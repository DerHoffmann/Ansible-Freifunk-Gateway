{
  "dataPath": "../data/",
  "siteName": "{{freifunk.name}}",
  "mapSigmaScale": 0.5,
  "showContact": false,
  "maxAge": 14,
  "blinkingHours": 8,
  "mapLayers": [
    {
      "name": "OSM Mapnik",
      "url": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",
      "config": {
        "subdomains": "abc",
        "type": "osm",
        "attribution": "&copy; <a href=\"http://www.openstreetmap.org/copyright\">OpenStreetMap</a>",
        "maxZoom": 18
      }
    },
    { "name": "MapQuest",
      "url": "https://otile{s}-s.mqcdn.com/tiles/1.0.0/osm/{z}/{x}/{y}.jpg",
      "config": {
        "subdomains": "1234",
        "type": "osm",
        "attribution": "Tiles &copy; <a href=\"https://www.mapquest.com/\" target=\"_blank\">MapQuest</a>, Data CC-BY-SA OpenStreetMap",
        "maxZoom": 18
      }
    },
    {
      "name": "Stamen.TonerLite"
    }
  ],
  "siteNames": [
{% for domaene in domaenen|dictsort %}
    { "site": "{{freifunk.kurzname}}d{{domaene[0]}}", "name": "Dom√§ne {{domaene[0]}} - {{domaene[1].name}}" }{% if not loop.last %},{% endif %}
{% endfor %}
  ],
  "nodeInfos": [
    { "name": "Clientstatistik",
      "href": "https://freifunk-muensterland.de/grafana/dashboard/db/node-overview?var-node={NODE_ID}",
      "thumbnail": "https://freifunk-muensterland.de/grafana/render/dashboard-solo/db/node-overview?var-node={NODE_ID}&panelId=1&theme=light&width=600&height=300",
      "caption": "Knoten {NODE_ID}"
    }
  ]
}

