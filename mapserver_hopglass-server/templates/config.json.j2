{
  "receiver": {
    "receivers": [
      { "module": "announced",
        "config": {
          "interval": {
            "statistics": 60,
            "nodeinfo": 500
          }
        }
      },
      { "module": "aliases",
        "config": {
          "file": "./aliases.json"
        },
        "overlay": true
      }
    ],
    "ifaces": [
{% for domaene in domaenen|dictsort %}
      "bat{{domaene[0]}}"{% if not loop.last %},{% endif %}

{% endfor %}
    ],
    "storage": {
      "file": "./raw.json"
    },
    "purge": {
      "maxAge": 14
    }
  },
  "provider": {
    "offlineTime": 900
  },
  "webserver": {
    "port": 4000
  }
}
